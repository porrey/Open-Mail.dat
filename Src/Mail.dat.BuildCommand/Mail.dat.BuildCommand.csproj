<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<RootNamespace>Mail.dat.BuildCommand</RootNamespace>
		<Version>0.0.7</Version>
		<AssemblyVersion>0.0.7</AssemblyVersion>
		<FileVersion>0.0.7</FileVersion>
	</PropertyGroup>

	<ItemGroup>
		<Content Include="Services\Mail.dat.BuildCommand.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Humanizer.Core" Version="2.14.1" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
		<PackageReference Include="Spectre.Console" Version="0.53.0" />
		<PackageReference Include="Diamond.Core.CommandLine" Version="9.2.0" />
		<PackageReference Include="Diamond.Core.CommandLine.Abstractions" Version="9.2.0" />
		<PackageReference Include="Diamond.Core.CommandLine.Model" Version="9.2.0" />
		<PackageReference Include="Diamond.Core.Extensions.DependencyInjection.Abstractions" Version="9.2.0" />
		<PackageReference Include="Diamond.Core.Extensions.Hosting.Abstractions" Version="9.2.0" />
		<PackageReference Include="Diamond.Core.System.TemporaryFolder" Version="9.2.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.10" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Mail.dat.Json.Specification\Mail.dat.Json.Specification.csproj" />
		<ProjectReference Include="..\Mail.dat.Abstractions\Mail.dat.Abstractions.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Content Update="$(UserProfile)\.nuget\packages\diamond.core.commandline.abstractions\9.2.0\contentFiles\any\net9.0\Services\Diamond.Core.CommandLine.Abstractions.json">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	</ItemGroup>

</Project>
